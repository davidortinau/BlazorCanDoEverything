<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:shared="clr-namespace:BlazorCanDoEverything.Shared;assembly=BlazorCanDoEverything.Shared"
             x:Class="BlazorCanDoEverything.Pages.ProjectsPage"
             Title="Projects">

    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html" StartPath="/projects">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type shared:Routes}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>
